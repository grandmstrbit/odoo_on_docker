<odoo>
			
    <record id="estate_property_tags_tree" model="ir.ui.view">
        <field name="name">estate.property.tags.tree</field>
        <field name="model">estate.property.tags</field>
        <field name="arch" type="xml">
            <tree string="Tags">
                <field name="name"/>
             
            </tree>
        </field>
    </record>
	
     <record id="estate_property_tags_form" model="ir.ui.view">
        <field name="name">estate.property.tags.form</field>
        <field name="model">estate.property.tags</field>
        <field name="arch" type="xml">
            <form string="Property Tags">
	            <sheet>
	                <group>
	                	<field name="name"/>
                        
	                </group>
	            </sheet>
            </form>
        </field>
    </record>    




	<record id="action_estate_property_tags" model="ir.actions.act_window">
		<field name="name">Property Tags</field>
		<field name="res_model">estate.property.tags</field>
		<field name="view_mode">tree,form</field>
	</record>



</odoo>