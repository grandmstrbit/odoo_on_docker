<odoo>
	<record id="view_sro_contacts_contract_form" model="ir.ui.view">
	    <field name="name">sro.contacts.contract.form</field>
	    <field name="model">sro.contacts.contract</field>
	    <field name="arch" type="xml">
	        <form string="SRO Contacts Contract">
	            <group>
	                <field name="tender_type" string="Предложение или поиск подряда:"/>
	                <field name="tender_description" string="Описание подряда:"/>
	                <field name="tender_contact_info" string="Контактные данные:"/>
	                <field name="tender_member_number" string="Регистрационный номер члена:"/>
	                <field name="tender_short_name" string="Сокращенное наименование организации:"/>  
	            </group>

	            <xpath expr="//field[@name='partner5_id']" position="replace"/>
	        </form>
	    </field>
	</record>
</odoo>